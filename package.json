    "angular-ui-bootstrap": "^2.3.1",
    "angular-ui-router": "^0.2.17",
    "angular-sanitize": "^1.5.3",
    "angular-ui-select": "^0.12.100",
    "bootstrap": "^3.3.6",
    "font-awesome": "^4.6.1",
    "paper-css": "^0.1.2",
    "lodash": "^4.17.2"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.0",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
